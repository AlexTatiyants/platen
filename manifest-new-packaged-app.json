{
  "name": "platen",
  "description": "Markdown WordPress Editor",
  "version": "1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
      "storage",
      "unlimitedStorage",
      "clipboardRead",
      "clipboardWrite",
      "http://*/",
      "https://*/"
  ],
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  },
  "sandbox": {
    "pages": ["views/embedded_ga_host.html"]
  }
}