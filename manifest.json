{
  "name": "Pltn",
  "description": "Markdown WordPress Editor",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  }
}